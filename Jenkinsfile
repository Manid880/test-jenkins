            steps {
                echo "dev Pipleline"
            }
        }
        stage ('testdeploy') {
            steps {
                echo "Test Pipleline"
            }
        }
        stage ('stagedeploy') {
            steps {
                echo "stage Pipleline"
            }
        }
        stage ('proddeploy') {
            steps {
                echo "Prod Pipleline"
            }
        }
    }
